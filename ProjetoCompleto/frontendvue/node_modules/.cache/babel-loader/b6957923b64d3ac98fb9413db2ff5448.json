AE,KAAI,CAAC;AANd,iBADL;;AASR,oBAAI,KAAI,CAAC,QAAL,IAAiB,KAAI,CAAC,gBAAvB,IAA6C,KAAI,CAAC,QAAL,CAAc,MAAd,IAAwB,CAAxE,EAA2E;AACvE,sBAAG,KAAI,CAAC,QAAL,IAAiB,KAAI,CAAC,gBAAzB,EAA2C;AACrC,oBAAA,SADqC,GACzB,6BADyB;AAE3C,mBAFA,MAGI;AACA,oBAAA,SAAQ,GAAI,EAAZ;AACJ;;AACA,sBAAG,KAAI,CAAC,QAAL,CAAc,MAAd,GAAuB,CAA1B,EAA6B;AAC3B,oBAAA,SAAQ,GAAI,SAAQ,GAAI,6BAAxB;AACF;AACJ;;AAnBQ,oBAoBJ,SApBI;AAAA;AAAA;AAAA;;AAqBJ,gBAAA,KAAI,CAAC,SAAL,GAAiB,EAAjB;AArBI;AAAA,uBAuBmB,KAAK,CAAC,IAAN,CAAW,YAAX,EAAyB,IAAzB,CAvBnB;;AAAA;AAuBE,gBAAA,QAvBF;;AAwBJ,oBAAG,QAAQ,CAAC,MAAT,IAAmB,GAAtB,EAA0B;AACtB;AACA,kBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,YAAlB;AACJ,iBAHA,MAIK;AACD,kBAAA,OAAO,CAAC,KAAR,CAAc,2CAAd;AACJ;;AA9BI;AAAA;;AAAA;AAiCJ,gBAAA,KAAI,CAAC,SAAL,GAAiB,SAAjB;;AAjCI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmCZ;AApCK;AAdE,CAAf","sourcesContent":["<template>\r\n    <div class=\"Register\">\r\n        <form @submit.prevent=\"DataForm\">\r\n           <div class=\"form-group\">\r\n              <p class=\"h2 text-secondary\">Novo Usuário</p>\r\n           </div>\r\n           <div class=\"mb-3\">\r\n                <label for=\"formGroupExampleInput\" class=\"form-label\">Nome</label>\r\n                <input type=\"text\" class=\"form-control form-control-sm\" v-model=\"name\" placeholder=\"Nome\" required>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n                <label for=\"formGroupExampleInput\" class=\"form-label\">Email</label>\r\n                <input type=\"text\" class=\"form-control\" v-model=\"email\" placeholder=\"Email\" required>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n                <label for=\"formGroupExampleInput\" class=\"form-label\">Telefone</label>\r\n                <input type=\"text\" class=\"form-control\" v-model=\"phone\" placeholder=\"Telefone\" required>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n                <label for=\"formGroupExampleInput\" class=\"form-label\">Endereço</label>\r\n                <input type=\"text\" class=\"form-control\" v-model=\"adress\" placeholder=\"Endereço\" required>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n                <label for=\"formGroupExampleInput\" class=\"form-label\">Senha</label>\r\n                <input type=\"password\" class=\"form-control form-control-pw\" v-model=\"password\" placeholder=\"Senha\" required>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n                <label for=\"formGroupExampleInput2\" class=\"form-label\">Confirme Senha</label>\r\n                <input type=\"password\" class=\"form-control form-control-pw\" v-model=\"password_confirm\" placeholder=\"Repita a Senha\" required>\r\n            </div>\r\n            <div>\r\n                <p v-if=\"erroSenha\" class=\"text-danger\">{{ erroSenha }}</p>\r\n                <button class=\"btn btn-primary btn-block\"> Registrar</button>\r\n            </div>\r\n        </form>\r\n    </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios'\r\n\r\n    export default {\r\n        name: 'Register',\r\n        data(){\r\n            return {\r\n                name : '',\r\n                email: '',\r\n                phone: '',\r\n                adress: '',\r\n                password: '',\r\n                password_confirm: '',\r\n                erroSenha : '',\r\n                senhaMenos6: '',\r\n            }\r\n        },\r\n        methods: {\r\n        async DataForm(){\r\n                const form = {\r\n                    name : this.name,\r\n                    email: this.email,\r\n                    phone: this.phone,\r\n                    adress: this.adress,\r\n                    password: this.password,\r\n                    password_confirm: this.password_confirm\r\n                }\r\n                if((this.password != this.password_confirm) || (this.password.length <= 6)){\r\n                    if(this.password != this.password_confirm ){\r\n                      var erroSenha = \"- A senha precisa ser igual\";\r\n                    }\r\n                    else{\r\n                        erroSenha = \"\";\r\n                    }\r\n                    if(this.password.length < 6 ){\r\n                      erroSenha = erroSenha + ' - Ter no míninmo 6 digitos';\r\n                    }\r\n                }\r\n                if(!erroSenha){\r\n                    this.erroSenha = \"\";\r\n\r\n                    const response = await axios.post('main/store', form)\r\n                    if(response.status == 200){\r\n                        //console.log(response);\r\n                        this.$router.push('/ListUsers');\r\n                    }\r\n                    else {\r\n                        console.error(\"Ocorreu um erro no API de Inserir Usuário\");\r\n                    }\r\n                }\r\n                else{\r\n                    this.erroSenha = erroSenha;\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.Register {\r\n\r\n    display: flex;\r\n    justify-content: center;\r\n\r\n}\r\n.form-control-sm {\r\n    width: 20rem;\r\n}\r\n.form-control-pw {\r\n    width: 10rem;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}